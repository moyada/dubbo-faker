<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byName" default-lazy-init="true">

    <bean id="batchRecordListener" class="cn.moyada.dubbo.faker.api.listener.BatchRecordListener">
        <property name="capacity" value="${faker.capacity}"/>
        <property name="maxThread" value="${faker.maxThread}"/>
    </bean>

    <bean id="fakerManager" class="cn.moyada.dubbo.faker.api.manager.FakerManager"/>
</beans>
