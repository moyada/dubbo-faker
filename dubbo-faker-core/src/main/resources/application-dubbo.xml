<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    					http://www.springframework.org/schema/beans/spring-beans.xsd
    					http://code.alibabatech.com/schema/dubbo
    					http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <import resource="application-dubbo-import.xml"/>
    <import resource="application-dubbo-export.xml"/>

    <context:property-placeholder order="0" location="classpath:/properties/dubbo.properties" />

    <dubbo:application name="${dubbo.application}" />
    <dubbo:application name="dubbo-faker" />
    <dubbo:registry address="${dubbo.registry}" backupProvider="${dubbo.provider.backup}"/>
    <dubbo:provider export="${dubbo.export}" token="${dubbo.token}"/>
    <dubbo:protocol name="dubbo" port="-1" host="intranet" />
    <dubbo:monitor protocol="registry"/>
    <dubbo:consumer timeout="3000" />
</beans>